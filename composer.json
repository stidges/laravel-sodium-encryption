{
    "name": "stidges/laravel-sodium-encrypter",
    "type": "library",
    "description": "A Sodium based encrypter class for Laravel",
    "keywords": [
        "laravel",
        "sodium",
        "libsodium",
        "encryption",
        "encrypter"
    ],
    "homepage": "https://github.com/stidges/laravel-sodium-encryption",
    "license": "MIT",
    "authors": [
        {
            "name": "Stidges",
            "email": "info@stidges.com",
            "homepage": "https://stidges.com"
        }
    ],
    "require": {
        "ext-sodium": "*",
        "php": ">=7.2",
        "illuminate/support": "~5.7|~5.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Stidges\\LaravelSodiumEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stidges\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stidges\\LaravelSodiumEncryption\\EncryptionServiceProvider"
            ]
        }
    }
}
